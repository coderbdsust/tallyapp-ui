{
  "name": "tallyapp-ui",
  "displayName": "Tally App",
  "version": "0.9.1",
  "description": "Angular & Tailwind CSS Admin Dashboard Starter Kit, Free and Open Source",
  "homepage": "https://github.com/coderbdsust/tallykhata-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbdsust/tallykhata-ui.git"
  },
  "keywords": [
    "angular",
    "tailwind",
    "starter-kit",
    "tailwind-template",
    "dark mode",
    "open source"
  ],
  "author": "Biswajit Debnath <biswajit.sust@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:e2e": "npx playwright test --ui",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:verify": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:staged": "pretty-quick --staged",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.4",
    "@angular/common": "^19.2.4",
    "@angular/compiler": "^19.2.4",
    "@angular/core": "^19.2.4",
    "@angular/forms": "^19.2.4",
    "@angular/platform-browser": "^19.2.4",
    "@angular/platform-browser-dynamic": "^19.2.4",
    "@angular/router": "^19.2.4",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@ng-select/ng-select": "^13.9.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "angular-svg-icon": "^13.0.0",
    "apexcharts": "^3.35.3",
    "chart.js": "^4.4.7",
    "check-password-strength": "^2.0.10",
    "flowbite": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "ng-apexcharts": "^1.7.1",
    "ngx-sonner": "^2.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.4",
    "@playwright/test": "^1.42.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.7",
    "cz-conventional-changelog": "^3.3.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.1.6",
    "typescript": "~5.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
